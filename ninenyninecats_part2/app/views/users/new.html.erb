<h1>New User!</h1>


<form action="<%= users_url %>" method="POST">

   
    <label>Username
        <input type="text" name="user[username]" value="<%= @user.user_name %>">
    </label>

    <label>Password
        <input type="password" name="user[password]" value="">
    </label>



    <button>Create User</button>

</form>